{
  "expo": {
    "name": "Seat Reservation",
    "slug": "seat-reservation-mobile",
    "version": "1.0.0",
    "owner": "digitalholdingsgroup",
    "description": "Mobile app for the Seat Reservation System. Created by DigitalHoldingsGroup LLC. All rights reserved.",
    "githubUrl": "https://github.com/RedClaus/Seatreservation",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0052CC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.seatreservation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0052CC"
      },
      "package": "com.company.seatreservation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
